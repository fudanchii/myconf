[ui]
username = Nurahmadie <nurahmadie@gmail.com>
verbose = true

[extensions]
rebase =
mq =
churn =
graphlog =

[bookmarks]
track.current = True

[alias]
st = status
up = update
co = update
cl = clone
ci = commit
pl = pull
ps = push
rb = rebase
br = branch
bs = branches
bm = bookmark
bms = bookmarks
book = bookmark
books = bookmarks
lp = log --graph --template '{rev}:{node|short}@{branch}\t[{phase}] {desc}\n'


[tortoisehg]
editor = gvim
